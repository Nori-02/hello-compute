    },
    "optionalDependencies": {
        "@fastly/cli-darwin-arm64": "=10.19.0",
        "@fastly/cli-darwin-x64": "=10.19.0",
        "@fastly/cli-linux-arm64": "=10.19.0",
        "@fastly/cli-linux-x64": "=10.19.0",
        "@fastly/cli-linux-x32": "=10.19.0",
        "@fastly/cli-win32-arm64": "=10.19.0",
