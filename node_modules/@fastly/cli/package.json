{
    "name": "@fastly/cli",
    "version": "10.19.0",
    "description": "Build, deploy and configure Fastly services from your terminal",
    "type": "module",
    "scripts": {
        "prepack": "cp ../../../README.md ../../../LICENSE ../../../SECURITY.md .",
        "version": "node ./update.js $npm_package_version"
    },
    "devDependencies": {
        "decompress": "^4.2.1",
        "decompress-targz": "^4.1.1"
